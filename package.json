{
  "name": "r-row",
  "version": "2.2.0",
  "description": "TypeScript 2D Game Engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "rm -rf ./dist && webpack --watch --devtool hidden-source-map",
    "build": "rm -rf ./dist && webpack --mode production",
    "publish": "rm -rf ./dist && npm run build && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheMenerv/r-row.git"
  },
  "keywords": [
    "game",
    "engine",
    "typescript",
    "canvas",
    "2d"
  ],
  "author": "Yoan B. (Menerv)",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/TheMenerv/r-row/issues"
  },
  "homepage": "https://github.com/TheMenerv/r-row#readme",
  "devDependencies": {
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  }
}
